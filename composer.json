{
    "name": "sethsharp/laravel-planable",
    "description": "Allows any model to be planable",
    "type": "composer-plugin",
    "license": "MIT",
    "homepage": "https://github.com/SethSharp/laravel-planable",
    "authors": [
        {
            "name": "Seth Sharp",
            "email": "sesharp@outlook.com"
        }
    ],
    "keywords": [
        "Laravel",
        "Planable",
        "Package"
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sethsharp\\LaravelPlanable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SethSharp\\LaravelPlanable\\PlanableServiceProvider"
            ]
        }
    }
}
